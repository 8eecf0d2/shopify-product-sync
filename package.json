{
  "name": "shopify-product-sync",
  "version": "0.0.0",
  "description": "Shopify product sync utility",
  "main": "dist/server.js",
  "author": "8eecf0d2",
  "license": "ISC",
  "scripts": {
    "lint": "tslint --project .",
    "watch": "webpack --watch --config ./webpack.config.js",
    "watch:dev": "NODE_ENV=development yarn run watch",
    "watch:client": "webpack --config ./src/client/webpack.config.js --watch --info-verbosity=verbose",
    "watch:server": "webpack --config ./src/server/webpack.config.js --watch --info-verbosity=verbose",
    "build": "webpack --config ./webpack.config.js",
    "build:dev": "NODE_ENV=development yarn run watch",
    "build:client": "webpack --config ./src/client/webpack.config.js --info-verbosity=verbose",
    "build:server": "webpack --config ./src/server/webpack.config.js --info-verbosity=verbose",
    "start": "NODE_ENV=development node dist/server.js",
    "start:dev": "NODE_ENV=development yarn run start",
    "monitor": "nodemon dist/server.js",
    "monitor:dev": "NODE_ENV=development nodemon dist/server.js"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/react": "^16.4.13",
    "@types/react-dom": "^16.0.7",
    "@types/react-router-dom": "^4.3.0",
    "@types/serve-static": "^1.13.2",
    "@types/webpack": "^4.4.11",
    "ts-loader": "^5.0.0",
    "typescript": "^3.0.3",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "@shopify/polaris": "^2.8.0",
    "express": "^4.16.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-router-dom": "^4.3.1"
  }
}
